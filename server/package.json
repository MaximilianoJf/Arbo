{
  "name": "rest_api_node_ts_server",
  "version": "1.0.0",
  "description": "Rest API's cn Express y Typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx sequelize-cli db:migrate && nodemon --exec tsx src/index.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:status": "npx sequelize-cli db:migrate:status",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "author": "Maximiliano Jimenez Flores",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  }
}